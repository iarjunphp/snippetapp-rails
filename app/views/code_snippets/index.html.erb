<p id="notice"><%= notice %></p>

<h1>Listing Code Snippets</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Code</th>
      <th>User</th>
      <th>Created at</th>
      <th>Updated at</th>
      <th>Created by</th>
      <th>Updated by</th>
      <th>Deleted at</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @code_snippets.each do |code_snippet| %>
      <tr>
        <td><%= code_snippet.title %></td>
        <td><%= code_snippet.description %></td>
        <td><%= code_snippet.code %></td>
        <td><%= code_snippet.user_id %></td>
        <td><%= code_snippet.created_at %></td>
        <td><%= code_snippet.updated_at %></td>
        <td><%= code_snippet.created_by %></td>
        <td><%= code_snippet.updated_by %></td>
        <td><%= code_snippet.deleted_at %></td>
        <td><%= code_snippet.status %></td>
        <td><%= link_to 'Show', code_snippet %></td>
        <td><%= link_to 'Edit', edit_code_snippet_path(code_snippet) %></td>
        <td><%= link_to 'Destroy', code_snippet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Code snippet', new_code_snippet_path %>
